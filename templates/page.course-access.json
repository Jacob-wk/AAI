{
  "sections": {
    "hero": {
      "type": "aai-universal-hero",
      "settings": {
        "hero_title": "{{ page.title | default: 'Course Access' }}",
        "hero_subtitle": "Continue your professional development journey",
        "background_style": "bg-light"
      }
    },
    "course_player": {
      "type": "aai-universal-content",
      "blocks": {
        "intuto_player": {
          "type": "text_block",
          "settings": {
            "content": "<div class='intuto-player-container'><div id='intuto-course-player'><p>Loading course content...</p></div></div>"
          }
        }
      },
      "block_order": [
        "intuto_player"
      ],
      "settings": {
        "layout_style": "single-column",
        "background_style": "bg-light"
      }
    },
    "navigation": {
      "type": "aai-universal-content",
      "blocks": {
        "nav_links": {
          "type": "text_block",
          "settings": {
            "content": "<div class='course-navigation'><div class='nav-item'><a href='/account' class='btn btn-outline'>‚Üê Back to Dashboard</a></div><div class='nav-item'><a href='/collections/courses' class='btn btn-outline'>Browse More Courses</a></div></div>"
          }
        }
      },
      "block_order": [
        "nav_links"
      ],
      "settings": {
        "layout_style": "single-column",
        "background_style": "bg-light"
      }
    },
    "support": {
      "type": "aai-universal-content",
      "blocks": {
        "support_info": {
          "type": "text_block",
          "settings": {
            "title": "Need Help?",
            "content": "<p>If you experience any technical issues with the course player, please contact our support team.</p><a href='/pages/help-center' class='btn btn-primary'>Get Support</a>"
          }
        }
      },
      "block_order": [
        "support_info"
      ],
      "settings": {
        "layout_style": "single-column",
        "background_style": "bg-primary"
      }
    }
  },
  "order": [
    "hero",
    "course_player",
    "navigation",
    "support"
  ]
}
