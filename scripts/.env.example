# AAI LMS - Environment Configuration
# Copy this file to .env and fill in your actual values

# Shopify Store Configuration
SHOPIFY_STORE_URL=https://your-store.myshopify.com
SHOPIFY_ACCESS_TOKEN=your_admin_api_access_token

# Shopify Webhook Configuration  
SHOPIFY_WEBHOOK_SECRET=your_webhook_secret

# Code Selling App Configuration
CODE_SELLING_APP_API_KEY=your_code_selling_app_api_key
CODE_SELLING_APP_SECRET=your_app_secret

# Intuto Integration Configuration
INTUTO_API_KEY=your_intuto_api_key
INTUTO_BASE_URL=https://api.intuto.com

# Analytics (Optional)
ANALYTICS_WEBHOOK_URL=your_analytics_webhook_url
ANALYTICS_API_KEY=your_analytics_api_key

# Development
NODE_ENV=development
